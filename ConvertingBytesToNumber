pragma solidity ^0.8.0;

contract ConvertingBytesToNumber {
    function bytesToUint(string memory b) public pure returns (uint256){
        uint rand;
        rand = uint(keccak256(abi.encodePacked(_str))) % 10000000000000000; //Zeros depending on how many digits you need
        return rand;
    return number;
    }
}
