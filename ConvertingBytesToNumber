pragma solidity ^0.8.0;

contract ConvertingBytesToNumber {
    function bytesToUint(string memory b) public pure returns (uint256){
        bytes32 c = keccak256(abi.encodePacked(b));
        uint256 number;
        for(uint i=0;i<c.length;i++){
            number = number + uint(uint8(c[i]))*(2**(8*(c.length-(i+1))));
        }
    return number;
    }


}
